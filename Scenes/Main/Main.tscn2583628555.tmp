func _ready():
	var tilemap_size_in_cells = $Farm.get_world_size()
	var cell_size = $Farm.get_cell_size()
	print(\"TileMap size in cells: \", tilemap_size_in_cells)
	var tilemap_size_in_world_coords = Vector2(
		tilemap_size_in_cells.size.x * cell_size.x,
		tilemap_size_in_cells.size.y * cell_size.y
	)
	print(\"TileMap size in world coordinates: \", tilemap_size_in_world_coords)
	print(\"Limit Left: \", tilemap_size_in_cells.position.x * cell_size.x)
	print(\"Limit Top: \", tilemap_size_in_cells.position.y * cell_size.y)
